{
  "name": "lethean-server",
  "version": "3.1.0",
  "license": "EUPL-1.2",
  "description": "Lethean VPN Node",
  "homepage": "https://github.com/letheanVPN/lethean-server/wiki",
  "bugs": {
    "url": "https://github.com/letheanVPN/lethean-server/issues",
    "email": "hello@lt.hn"
  },
  "author": "Snider <snider@lt.hn> (Snider.lthn)",
  "main": "mod.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/letheanVPN/lethean-server.git"
  },
  "scripts": {
    "dev": "deno run --watch -A --unstable mod.ts",
    "dev:gui": "deno run --watch -A --unstable --import-map vendor/import_map.json mod.ts gui",
    "dev:setup": "cd $HOME/Lethean/conf && git clone https://github.com/letheanVPN/config-templates.git templates",
    "start": "deno run -A --unstable --import-map vendor/import_map.json mod.ts",
    "server:compile": "deno compile --unstable -A --output build/lthn --config tsconfig.json  --import-map vendor/import_map.json --target  ",
    "server:build:linux": "npm run server:compile -- x86_64-unknown-linux-gnu mod.ts",
    "server:build:macos-intel": "npm run server:compile -- x86_64-apple-darwin mod.ts",
    "server:build:macos-arm": "npm run server:compile -- aarch64-apple-darwin mod.ts",
    "server:build:windows": "npm run server:compile -- x86_64-pc-windows-msvc mod.ts",
    "test": "deno test --coverage=cov -A --unstable --config tsconfig.json --import-map vendor/import_map.json --doc test/ ",
    "fmt": "deno fmt -c deno.json",
    "sdk:angular": "openapi-generator generate -i openapi.json -g typescript-angular -o build/api"
  },
  "dependencies": {
    "openpgp": "^5.2.1",
    "pem-ts": "^3.0.0"
  },
  "devDependencies": {
    "typescript": "^4.6.3",
    "typescript-deno-plugin": "^1.31.0"
  }
}
